{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": [
                "PYTHONDONTWRITEBYTECODE=1",
                "-p no:cacheprovider"
            ],
            "console": "integratedTerminal"
        }
    ]
}